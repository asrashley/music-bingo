{
  "name": "musicalbingo",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@asrashley/multipart-stream": "git+https://github.com/asrashley/multipart-stream-js.git",
    "@hookform/error-message": "^2.0.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "connected-react-router": "^6.9.3",
    "enumify": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "file-saver": "^2.0.2",
    "git-rev-webpack-plugin": "^0.3.1",
    "loglevel": "^1.8.1",
    "named-urls": "^2.0.0",
    "node-sass": "^7.0.3",
    "react": "^16.13.1",
    "react-circular-progressbar": "^2.0.3",
    "react-datetime-picker": "^4.1.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^7.28.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "react-widgets": "^4.5.0",
    "redux-logger": "^3.0.6",
    "rsuite-table": "^5.6.1",
    "validator": "^13.7.0"
  },
  "scripts": {
    "start": "node src/react-scripts.js start",
    "build": "node src/react-scripts.js build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@asrashley|node-fetch|data-uri-to-buffer|fetch-blob|formdata-polyfill)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!src/index.js",
      "!src/mocks/*.js",
      "!src/serviceWorker.js",
      "!src/react-scripts.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 75,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "fetch-mock-jest": "^1.5.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-extended": "^3.2.4",
    "react-test-renderer": "^16.13.1",
    "stream": "^0.0.2",
    "wait-for-expect": "^3.0.2"
  }
}
